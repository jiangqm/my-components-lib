{
  "name": "my-components-lib",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "author": "",
  "license": "MIT",
  "main": "./dist/my-components-lib.umd.cjs",
  "module": "./dist/my-components-lib.js",
  ".": {
    "import": "./dist/my-components-lib.js",
    "require": "./dist/my-components-lib.umd.js"
  },
  "types": "./dist/types",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "dev:doc": "vite serve docs",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"git commit trigger husky pre-commit hook\" "
    }
  },
  "lint-staged": {
    "src/**/*.{js,tsx,ts}": [
      "prettier --write",
      "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@swc/core-win32-x64-msvc": "^1.3.86",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@mdx-js/react": "^2.3.0",
    "@rollup/plugin-typescript": "^11.1.3",
    "@swc/wasm": "^1.3.86",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-pages-theme-doc": "^4.1.6",
    "vite-plugin-react-pages": "^4.1.4",
    "vitest": "^0.34.4"
  }
}
